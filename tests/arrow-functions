var test = require('tape');
var util = require('./util');

test.createStream()
  .pipe(require('tap-spec'))
  .pipe(process.stderr);

test('Simple Arrow Functions test', function (t) {
  util.patchLogger();

  t.plan(1);

  t.equal(util.getTestResult(() => 1), '[Function "Anonymous"]');

  util.restoreLogger();
});
